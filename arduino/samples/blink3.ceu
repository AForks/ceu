constant _OUTPUT, _LOW, _HIGH;

output int PIN11;
output int PIN12;
output int PIN13;

par do
    loop do
        emit PIN11(_HIGH);
        await 1s;
        emit PIN11(_LOW);
        await 1s;
    end
with
    loop do
        emit PIN12(_HIGH);
        await 500ms;
        emit PIN12(_LOW);
        await 500ms;
    end
with
    loop do
        emit PIN13(_HIGH);
        await 250ms;
        emit PIN13(_LOW);
        await 250ms;
    end
end
