input int PIN02_LOW;

_pinMode(13, _OUTPUT);

loop do
    par/or do
        u64 t = 1000;
        loop do
            _digitalWrite(13, _LOW);
            await (t*1000LL);
            _digitalWrite(13, _HIGH);
            await (t*1000LL);
            if t > 50 then
                t = t - 10;
            end
        end
    with
        await PIN02_LOW;
    end
end
