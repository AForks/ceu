input  int PIN02;
output int PIN13;

loop do
    par/or do
        int t = 1000;
        loop do
            emit PIN13(_LOW);
            await (t)ms;
            emit PIN13(_HIGH);
            await (t)ms;
            if t >= 200 then
                t = t - 100;
            end
        end
    with
        loop do
            int v = await PIN02;
            if v == _LOW then
                break;
            end
        end
    end
end
