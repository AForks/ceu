input int Start;
input int PIN02_LOW, PIN02_HIGH;

_pinMode(13, _OUTPUT);

u64 t = 500;
par/or do
    loop do
        _digitalWrite(13, _HIGH);
        await (t*1000);
        _digitalWrite(13, _LOW);
        await (t*1000);
    end
with
    loop do
        await PIN02_LOW;
        par/or do
            await PIN02_LOW;
            t = 500;
        with
            await 200ms;
            if t > 100 then
                t = t - 50;
            end
        end
    end
end
