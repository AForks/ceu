@include(bitmap.c);
@include(radio.m4);
@include(DS.m4);

@TOS_retry(200ms, @RADIO_start);
_DBG("Radio started\n");

@define(N_NODES, 16);

u8[ eval(N_NODES/8) ] neighs;

par/or do
    @DS_neighbours(100, 200ms, neighs, N_NODES);
with
    await 1s;
end

u8[255] all;
_bm_tostr(neighs, N_NODES, all);
_DBG("0123456789012345\n");
_DBG("%s\n\n", all);

return 0;
